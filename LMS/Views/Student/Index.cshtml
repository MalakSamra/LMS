@model IEnumerable<EnrolledCoursesViewModel>

@{
    ViewData["Title"] = "Index";
}

<h1>List Of Progress</h1>

<p>
    <a asp-action="Create" class="btn btn-outline-primary">Create</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Course Title
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.CourseTitle
                </td>
                <td>
                    <a asp-action="Details" asp-route-id="@item.Crs_Id" class="btn btn-outline-info">Details</a>
                </td>
            </tr>
        }
    </tbody>
</table>