@if (!User.Identity.IsAuthenticated)
{
    <li class="nav-item">
        <span class="nav-link text-dark">LogIn To Continue</span>
    </li>
}
else
{
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Account" asp-action="Logout">Log_Out:   @User.Identity.Name</a>
    </li>
}
